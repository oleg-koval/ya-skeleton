{
  "name": "ya_skeleton",
  "version": "0.0.0-development",
  "description": "Dockerized skeleton based on expressjs framework, with mongo support",
  "main": "index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": "oleg-koval/ya-skeleton",
  "scripts": {
    "start": "./bin/server | exec $LOGGER -o short",
    "start:rs": "nodemon ./bin/server",
    "test": "mocha",
    "test:docker": "docker-compose -f docker-compose.test.yml run tests",
    "docker:down": "docker-compose down",
    "lint": "eslint .",
    "ci": "yarn run lint && yarn run test:docker",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "release": {
    "publish": [
      "@semantic-release/github"
    ]
  },
  "author": "Oleg Koval",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.4",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-bunyan-logger": "^1.3.3",
    "express-request-id": "^1.4.1",
    "mongoose": "^5.5.5",
    "nodemon": "^1.18.7",
    "qs": "^6.7.0",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "danger": "^6.1.10",
    "eslint": "^5.7.0",
    "eslint-config-sexy": "^5.0.0",
    "mocha": "^9.0.3",
    "semantic-release": "^15.13.3",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "travis-deploy-once": "^5.0.11"
  },
  "snyk": true
}
