{
  "name": "ya_skeleton",
  "version": "0.0.0-development",
  "description": "Dockerized skeleton based on expressjs framework, with mongo support",
  "main": "index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": "oleg-koval/ya-skeleton",
  "scripts": {
    "start": "./bin/server | exec $LOGGER -o short",
    "start:rs": "nodemon ./bin/server",
    "test": "mocha",
    "test:docker": "docker-compose -f docker-compose.test.yml run tests",
    "docker:down": "docker-compose down",
    "lint": "eslint .",
    "ci": "yarn run lint && yarn run test:docker",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "release": {
    "publish": [
      "@semantic-release/github"
    ]
  },
  "author": "Oleg Koval",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-bunyan-logger": "^1.3.3",
    "express-request-id": "^1.4.1",
    "mongoose": "^5.4.13",
    "nodemon": "^1.18.7",
    "qs": "^6.6.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "danger": "^6.1.10",
    "eslint": "^5.7.0",
    "eslint-config-sexy": "^5.0.0",
    "mocha": "^5.2.0",
    "semantic-release": "^15.13.3",
    "should": "^13.2.3",
    "supertest": "^3.4.2",
    "travis-deploy-once": "^5.0.11"
  }
}
